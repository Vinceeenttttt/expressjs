<h1 class="text-center mb-3">{{title}}</h1>
<form action="/api/members" method="POST" class="mb-4">
  <div class="form-group">
    <label for="name">Please enter your URL link below:</label>
    <input type="url" name="url" class="form-control">
  </div>
  <input type="submit" value="Play" class="btn btn-primary btn-block">
</form>
<h4>Video:</h4>
  <div class="wrapper">
    {{#each members}}
        <div style="max-width:854px"><div style="position:relative;height:0;padding-bottom:56.25%"><iframe src="{{this.url}}" width="854" height="480" style="position:absolute;left:0;top:0;width:100%;height:100%" frameborder="0" scrolling="no" allowfullscreen></iframe></div></div>
    {{/each}}
  </div>

<a href="/api/members" class="btn btn-light mt-4">Visit API</a>